<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>CaptionToken</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <types>
        <typeDescription>
            <name>captionTokenExtractor.type.ViewMappingToken</name>
            <description>Token that holds the begin and end in another CAS View</description>
            <supertypeName>uima.tcas.Annotation</supertypeName>
            <features>
                <featureDescription>
                    <name>ValueSourceView</name>
                    <description>The surface value of the Token in the Source View</description>
                    <rangeTypeName>uima.cas.String</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>BeginSourceView</name>
                    <description>Begin in the source view</description>
                    <rangeTypeName>uima.cas.Integer</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>EndSourceView</name>
                    <description>End in the source view</description>
                    <rangeTypeName>uima.cas.Integer</rangeTypeName>
                </featureDescription>
            </features>
        </typeDescription>
    </types>
</typeSystemDescription>
